; Question 1. a)
;[2 marks] 1 + -2 - 3 + 4 + -5 + 6

(+ (- 3) (+ (- 5) (+ (- 2) (+ ( + 1 4 ) 6))))


; Question 1. b)
; 20-1+((26/2 + 2)*(20/5 - 2))
( + ( * ( + ( / 26 2 ) 2 ) ( + ( / 20 5 ) (- 2) ) ) ( + 20 (- 1) ) )

; Question 2. b)
;[2 marks] Create a procedure that computes the following function:
;	g(x) = f(2x) - 2x^3
(define ( f x ) ( + ( * ( * x x ) 3 ) 4 ) )

; Question 3. a)
(define (quadratic a b c) ( / ( + (* -1 b)
                                  (sqrt ( - ( * b b ) (* (* 4 a) c) ) ) )
                              (* 2 a) ) )

